        function BdToggleRow(ArregloFilas, IdUop) {

            console.table(ArregloFilas)

            var Filas = ArregloFilas.split(', ');
            Filas = Filas.map(function (equipo) {
                return equipo.trim();
            });

            var rowToToggle = document.getElementById(`Bd_${Filas[0]}`);

            if (rowToToggle.classList.contains('d-none')) {
                Filas.forEach(function (Fila) {
                    $(`#Bd_${Fila}`).removeClass('d-none');
                });

                $(`#Bd_${IdUop}`).removeClass('fa-plus-circle');
                $(`#Bd_${IdUop}`).addClass('fa-minus-circle');
            }
            else {
                Filas.forEach(function (Fila) {
                    $(`#Bd_${Fila}`).addClass('d-none');
                });

                $(`#Bd_${IdUop}`).addClass('fa-plus-circle');
                $(`#Bd_${IdUop}`).removeClass('fa-minus-circle');
            }
        }

