@section scripts {

    @*<script src="Metronic/plugins/custom/datatables/datatables.bundle.js"></script>*@

    @*Date picker*@
    <script>
        $(document).ready(function () {
            //Ocultar AsideMenu
            $("#kt_body").attr("data-kt-aside-minimize", "on");

            //Tablas flexibles
            $('.dataTables_scrollHeadInner').css('max-width', '100%');
            $('.dataTables_scrollHeadInner').css('min-width', '90%');
        })


        $("#kt_datatable_responsive").DataTable({
            responsive: true
        });


        function toggleRow() {
            const icon = document.getElementById('iconShow');
            const rowToToggle = document.getElementById('row1');

            if (rowToToggle.classList.contains('d-none')) {
                $("#row1").removeClass('d-none');
                $("#row2").removeClass('d-none');
                $("#row3").removeClass('d-none');

                $("#iconShow").removeClass('fa-plus-circle');
                $("#iconShow").addClass('fa-minus-circle');
            }
            else {
                $("#row1").addClass('d-none');
                $("#row2").addClass('d-none');
                $("#row3").addClass('d-none');

                $("#iconShow").addClass('fa-plus-circle');
                $("#iconShow").removeClass('fa-minus-circle');
            }
        }


    </script>

}
