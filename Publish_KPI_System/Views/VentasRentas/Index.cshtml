<div class="modal fade" tabindex="-1" id="ModalPartUsadas" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog mw-950px">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title"><i class="fas fa-boxes fs-1 text-warning me-3"></i><span id="PartidaId"> </span> </h3>
                <a class="btn btn-icon ms-2" data-bs-dismiss="modal" aria-label="Close">
                    <span class="svg-icon svg-icon-2x svg-icon-light text-hover-danger"><i class="fas fa-times fs-3"></i></span>
                </a>
            </div>
            <div class="modal-body">
                <div class="table-responsive">
                    <table id="TablaPartUsadas" class="table table-bordered">
                        <thead>
                            <tr class="fw-bold fs-6 text-gray-300 bg-opacity-20 bg-primary ">
                                <th class="min-w-200px">Folio</th>
                                <th class="min-w-50px">Cantidad</th>
                                <th class="min-w-50px">Precio</th>
                                <th class="min-w-50px">Importe</th>
                                <th class="min-w-50px">Moneda</th>
                                <th class="min-w-350px">Solicitante PMX</th>
                                <th class="min-w-350px">Autorizador PMX</th>
                                <th class="min-w-200px">Fecha</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-light p-3 fs-7 me-3" data-bs-dismiss="modal">Cerrar</button>
            </div>

        </div>
    </div>
</div>

